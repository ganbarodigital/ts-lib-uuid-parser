{
  "name": "@ganbarodigital/ts-lib-uuid-parser",
  "version": "0.3.0",
  "description": "Converts human-readable UUIDs to bytes",
  "repository": "github:ganbarodigital/ts-lib-uuid-parser",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "npm run clean && tsc",
    "lint": "tslint -p tsconfig.json",
    "prepublishOnly": "npm test && npm run lint",
    "prepack": "npm run build",
    "test": "npm run clean && tsc && mocha -r ts-node/register \"src/**/*.spec.ts\"",
    "cover": "rimraf coverage && rimraf .nyc_output && nyc --cache=false npm run test",
    "clean": "rimraf lib"
  },
  "contributors": [
    "Stuart Herbert <stuherbert@ganbarodigital.com> (https://ganbarodigital.com)"
  ],
  "keywords": [
    "uuid"
  ],
  "files": [
    "lib/**/*",
    "CHANGELOG.md",
    "LICENSE.md",
    "V1/**"
  ],
  "license": "BSD-3-Clause",
  "dependencies": {
    "@ganbarodigital/ts-lib-error-reporting": "^0.2.1",
    "@ganbarodigital/ts-lib-http-types": "^0.1.5",
    "@ganbarodigital/ts-lib-packagename": "^0.2.3",
    "@ganbarodigital/ts-lib-value-objects": "^1.1.1"
  },
  "devDependencies": {
    "@types/node": "^13.7.4",
    "tslint": "^5.20.1",
    "typescript": "^3.8.2",
    "@types/chai": "^4.2.9",
    "@types/mocha": "^7.0.1",
    "chai": "^4.2.0",
    "chai-bytes": "^0.1.2",
    "mocha": "^7.0.1",
    "nyc": "^15.0.0",
    "rimraf": "^3.0.2",
    "ts-node": "^8.0.0"
  }
}
