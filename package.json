{
  "name": "@ganbarodigital/ts-uuid-parser",
  "version": "0.0.1",
  "description": "Converts human-readable UUIDs to bytes",
  "repository": {
    "type": "git",
    "url": "https://github.com/ganbarodigital/ts-uuid-parser.git"
  },
  "main": "lib/V1/index.js",
  "types": "lib/V1/index.d.ts",
  "scripts": {
    "build": "tsc",
    "lint": "tslint -p ./tsconfig.json",
    "prepare": "npm run build",
    "test": "node_modules/.bin/tsc -p ./tsconfig-test.json && node_modules/.bin/jasmine-ts --config=\"src/spec/support/jasmine.json\" \"src/**/*.spec.ts\"",
    "cover": "node_modules/.bin/tsc -p ./tsconfig-test.json && nyc --cache=false node_modules/.bin/jasmine-ts --config=\"src/spec/support/jasmine.json\" \"src/**/*.spec.ts\"",
    "clean": "find src -name '*.js' | xargs rm && find src -name '*.js.map' | xargs rm",
    "postinstall": "npm run build"
  },
  "author": "",
  "files": [ "lib/**/*", "CHANGELOG.md", "LICENSE.md" ],
  "license": "BSD-3-Clause",
  "dependencies": {
    "@types/node": "^13.1.2",
    "tslint": "^5.20.1",
    "typescript": "^3.7.4"
  },
  "devDependencies": {
    "@types/jasmine": "^3.5.0",
    "jasmine": "^3.5.0",
    "jasmine-ts": "^0.3.0",
    "jasmine-ts-console-reporter": "^3.1.1",
    "nyc": "^15.0.0",
    "ts-node": "<8.0.0"
  }
}
