{
  "name": "@ganbarodigital/ts-uuid-parser",
  "version": "0.2.0",
  "description": "Converts human-readable UUIDs to bytes",
  "repository": "github:ganbarodigital/ts-uuid-parser",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "npm run clean && tsc -p ./tsconfig.json",
    "lint": "tslint -p ./tsconfig.json",
    "prepublishOnly": "npm test && npm run lint",
    "prepack": "npm run build",
    "test": "npm run clean && tsc -p ./tsconfig-test.json && jasmine --require=ts-node/register --config=\"src/spec/support/jasmine.json\" \"src/**/*.spec.ts\"",
    "cover": "rimraf coverage && rimraf .nyc_output && nyc --cache=false npm run test",
    "clean": "rimraf lib"
  },
  "contributors": [
    "Stuart Herbert <stuherbert@ganbarodigital.com> (https://ganbarodigital.com)"
  ],
  "keywords": [
    "uuid"
  ],
  "files": [
    "lib/**/*",
    "CHANGELOG.md",
    "LICENSE.md",
    "V1/**"
  ],
  "license": "BSD-3-Clause",
  "dependencies": {
    "@ganbarodigital/ts-lib-value-objects": "^1.0.0",
    "@ganbarodigital/ts-on-error": "0.0.2",
    "@types/node": "^13.1.8",
    "tslint": "^5.20.1",
    "typescript": "^3.7.5"
  },
  "devDependencies": {
    "@ganbarodigital/jasmine-ts": "file:../jasmine-ts",
    "@types/jasmine": "^3.5.1",
    "jasmine": "^3.5.0",
    "jasmine-ts-console-reporter": "^3.1.1",
    "nyc": "^15.0.0",
    "rimraf": "^3.0.0",
    "ts-node": "^8.6.2"
  }
}
